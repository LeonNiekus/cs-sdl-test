<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <ApplicationIcon />
    <OutputType>Exe</OutputType>
    <StartupObject />
  </PropertyGroup>

  <Target Name="PreBuild" BeforeTargets="PreBuildEvent">
    <Exec Command="XCOPY &quot;$(SolutionDir)..\API\bin\Debug\net5.0\API.dll&quot; &quot;$(OutDir)include\&quot; /Y&#xD;&#xA;XCOPY &quot;$(SolutionDir)..\CS-SDL-test\bin\Debug\net5.0\Lib.dll&quot; &quot;$(OutDir)include\&quot; /Y&#xD;&#xA;XCOPY &quot;$(SolutionDir)..\CS-SDL-test\post_build&quot; &quot;$(OutDir)include\&quot; /Y /S" />
  </Target>

  <ItemGroup>
    <Reference Include="API">
      <HintPath>bin\Debug\net5.0\include\API.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
